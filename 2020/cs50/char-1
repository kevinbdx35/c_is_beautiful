#include <stdio.h>

int main(void)
{
	char c1 = 'H'; //quand il n'y a qu'un seul caractère, il faut mettre des guillemets simples, en C il faut distingué les simples caractères des chaînes de caractères où il faut mettre des doubles guillemets
	char c2 = 'I';
	char c3 = '!';
	printf("%c %c %c\n", c1, c2, c3);
}
